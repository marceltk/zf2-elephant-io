{
  "name": "marceltk/elephant-io",
  "type": "library",
  "description": "Send events to a websocket real time engine though PHP",
  "keywords": [
    "Nodejs",
    "WebSocket",
    "Dialog",
    "Real Time"
  ],
  "license": "MIT",
  "homepage": "http://elephant.io",
  "authors": [
    {
      "name": "Marcel Silva",
      "email": "phpmarcel@gmail.com",
      "role": "Maintainer"
    },
    {
      "name": "Elephant.IO Community",
      "role": "Contributors :)"
    }
  ],
  "require": {
    "wisembly/elephant.io": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "ElephantIO\\": [
        "src/",
        "test/"
      ]
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
